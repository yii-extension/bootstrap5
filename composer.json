{
    "name": "yii-extension/bootstrap5",
    "type": "library",
    "description": "Yii Extension Twitter Bootstrap5.",
    "keywords": [
        "yii3",
        "extension",
        "twitter",
        "bootstrap5"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "yii-extension/asset-bootstrap5": "^1.0",
        "yiisoft/html": "^2.0",
        "yiisoft/widget": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.8",
        "vimeo/psalm": "^4.8",
        "yiisoft/var-dumper": "^1.1.0",
        "yiisoft/test-support": " ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Bootstrap5\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "infection/extension-installer": true,
            "foxy/foxy": true
        }
    }
}
